<?php

$url = "https://unifiplaytv.samsam123.tk/m3u8/unifi-tv?userID=0109194748&clientPasswd=0508timaH";

$curl = curl_init($url);

curl_setopt($curl, CURLOPT_URL, $url);

curl_setopt($curl, CURLOPT_RETURNTRANSFER, true);

//for debug only!

curl_setopt($curl, CURLOPT_SSL_VERIFYHOST, false);

curl_setopt($curl, CURLOPT_SSL_VERIFYPEER, false);

$resp = curl_exec($curl);

curl_close($curl);

var_dump($resp);

?>

